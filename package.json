{
  "name": "ai-web-core",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "cross-env NODE_ENV=production nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "migrate:create": "pnpm build && cross-env NODE_ENV=migration typeorm migration:create db/migrations/InitialMigration",
    "migrate:init": "pnpm build && cross-env NODE_ENV=migration typeorm migration:generate db/migrations/InitialMigration -d dist/db/dataSource.js",
    "migrate:gen": "pnpm build && cross-env NODE_ENV=migration typeorm migration:generate db/migrations/PostRefactoring -d dist/db/dataSource.js",
    "migrate:run": "pnpm build && cross-env NODE_ENV=migration typeorm migration:run -d dist/db/dataSource.js",
    "migrate:revert": "pnpm build && cross-env NODE_ENV=migration typeorm migration:revert -d dist/db/dataSource.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.11",
    "@nestjs/core": "^10.4.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.11",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "@types/bcrypt": "^5.0.2",
    "ali-oss": "^6.21.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "email-validator": "^2.0.4",
    "jsonschema": "^1.4.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.11",
    "@types/ali-oss": "^6.16.11",
    "@types/express": "^5.0.0",
    "@types/jest": "29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "22.10.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "29.7.0",
    "mysql2": "^3.11.4",
    "prettier": "^3.4.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
